#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/1. WEB DEVELOPMENT/BATCH57/Learn_Next.js/step06_react_learning/001_basic_concepts/001_coding_examples/node_modules/.pnpm/husky@9.1.5/node_modules/husky/node_modules:/mnt/e/1. WEB DEVELOPMENT/BATCH57/Learn_Next.js/step06_react_learning/001_basic_concepts/001_coding_examples/node_modules/.pnpm/husky@9.1.5/node_modules:/mnt/e/1. WEB DEVELOPMENT/BATCH57/Learn_Next.js/step06_react_learning/001_basic_concepts/001_coding_examples/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/1. WEB DEVELOPMENT/BATCH57/Learn_Next.js/step06_react_learning/001_basic_concepts/001_coding_examples/node_modules/.pnpm/husky@9.1.5/node_modules/husky/node_modules:/mnt/e/1. WEB DEVELOPMENT/BATCH57/Learn_Next.js/step06_react_learning/001_basic_concepts/001_coding_examples/node_modules/.pnpm/husky@9.1.5/node_modules:/mnt/e/1. WEB DEVELOPMENT/BATCH57/Learn_Next.js/step06_react_learning/001_basic_concepts/001_coding_examples/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../husky/bin.js" "$@"
else
  exec node  "$basedir/../husky/bin.js" "$@"
fi
